<!DOCTYPE html>
<html>
  <head>
    <title>GameOfThrones</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav>
      <h1>Welcome to Game of Thrones!</h1>
      <h2><%= link_to "View Houses", houses_path %> | <%= link_to "View Characters", characters_path %> | <%= link_to "New Characters", new_character_path %></h2>
    </nav>

    <%= yield %>

  </body>
</html>
